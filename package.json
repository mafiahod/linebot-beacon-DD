{
  "name": "line-bot-nodejs-starter",
  "version": "1.0.0",
  "description": "starter point to create new line bot project",
  "main": "app.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "test": "npm run build && jasmine ./dist/src/core/test/service_spec/message_service_spec.js",
    "start": "npm run build && node dist/src/app.js"
  },
  "keywords": [
    "line",
    "bot",
    "messaging-api"
  ],
  "author": "Sitthi Thiammekha",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jasmine": "^3.4.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@line/bot-sdk": "^6.8.0",
    "babel": "^6.23.0",
    "babelify": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.13",
    "log4js": "^4.4.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1"
  }
}
